{
  "name": "emergence-engine",
  "version": "1.0.0",
  "description": "A simulation engine for emergent behaviors",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "original": "bash scripts/launch-original.sh",
    "primal": "bash scripts/launch-primal.sh",
    "experimental": "bash scripts/launch-experimental.sh",
    "build": "vite build",
    "build:original": "vite build --config vite.config.port2000.js",
    "build:primal": "vite build --config vite.config.port3000.js",
    "build:experimental": "vite build --config vite.config.port4000.js",
    "serve": "vite preview"
  },
  "dependencies": {
    "pixi.js": "^7.4.3"
  },
  "devDependencies": {
    "vite": "^4.5.0"
  }
}
